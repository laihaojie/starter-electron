  "homepage": "./",
  "repository": "https://github.com/laihaojie/starter-electron",
  "main": "build/main/main.js",
  "scripts": {
    "postinstall": "node scripts/install.js",
    "dev": "premove build && node scripts/dev-server.js",
    "build": "premove build && node scripts/build.js",
    "build:all": "premove build && node scripts/build.js && electron-builder",
    "build:win": "pnpm run build && electron-builder --win --publish=never",
    "build:mac": "pnpm run build  && electron-builder --mac --publish=never",
    "build:linux": "pnpm run build && electron-builder --linux --publish=never",
    "release": "git add . && npx bumpp patch -y --commit --push --all --no-tag",
    "publish": "node scripts/publish.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\"",
    "posttest": "echo \"Error: 1111111111\""
  },
